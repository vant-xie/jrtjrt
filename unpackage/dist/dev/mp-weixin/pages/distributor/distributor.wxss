}
.distributor-footer{
	padding-left: 30rpx;
}
.distributor-footer-top{
	margin-top: 30rpx;
	font-size: 30rpx;
	color: #000000;
	position: relative;
	padding-left: 14rpx;
}
.distributor-footer-top::before{
	content: "";
	position: absolute;
	height: 30rpx;
	width: 4rpx;
	top: 6rpx;
	left: 0;
	border-left: 4rpx solid #FF3434;
}
