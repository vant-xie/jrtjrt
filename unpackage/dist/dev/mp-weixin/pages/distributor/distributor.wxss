	width: 750rpx;
	height: 360rpx;
}
.distributor-top>image {
	width: 100%;
	height: 100%;
}
.distributor-item {
	padding: 30rpx 0 0 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	background-color: #FFFFFF;
	/* margin-top: 2upx; */
}
.distributor-item>label {
	font-size: 30rpx;
	color: #333333;
}
.distributor-item>input {
	width: 78%;
	padding-left: 28rpx;
	font-size: 30rpx;
	color: #616161;
}
