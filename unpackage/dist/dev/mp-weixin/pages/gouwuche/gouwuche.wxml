<block wx:if="{{showF}}"><view class="gocart"><view class="gocart-main"><checkbox-group class="check" name="check" value="{{checkList}}" data-event-opts="{{[['change',[['changeitem',['$event']]]]]}}" bindchange="__e"><block wx:for="{{detriment}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{isShow==1}}"><view class="golist"><view class="golist-l"><checkbox value="{{index}}" checked="{{item.flag}}"></checkbox></view><view class="golist-t" data-id="{{item.goods_id}}" data-event-opts="{{[['tap',[['btn_detaxx',['$event']]]]]}}" bindtap="__e"><image src="{{item.pic}}"></image></view><view class="golist-r"><view class="golist-r1"><view class="golist-r1-l"><view class="golist-r-text1">{{item.goods_name}}</view><view class="golist-r-text2">{{"￥"+item.money}}</view></view><image src="../../static/img/shanchu.png" data-event-opts="{{[['tap',[['del',['$0',index],[[['detriment','',index]]]]]]]}}" bindtap="__e"></image></view><view class="golist-r2"><label data-event-opts="{{[['tap',[['btn_minute',[index]]]]]}}" class="minute" bindtap="__e">-</label><input class="count" type="text" disabled="true" data-event-opts="{{[['input',[['__set_model',['$0','num','$event',[]],[[['detriment','',index]]]]]]]}}" value="{{item.num}}" bindinput="__e"/><label data-event-opts="{{[['tap',[['btn_add',[index]]]]]}}" class="add" bindtap="__e">+</label></view></view></view></block></block></checkbox-group><block wx:if="{{isShow==0}}"><view class="gocart-lack"><view class="gocart-lack-img"><image src="../../static/img/gocart/gocart-lack.png"></image></view><text>购物车是空的哦</text></view></block><view class="gocart-footer"><view class="quanxuan"><checkbox-group data-event-opts="{{[['change',[['allchange',['$event']]]]]}}" class="allBtn" bindchange="__e"><checkbox value="cg" checked="{{allchecked}}"></checkbox>全选</checkbox-group></view><view class="zongjia"><text>总计：<text>{{"￥"+allprice}}</text></text></view><view data-event-opts="{{[['tap',[['jiesuan',[0]]]]]}}" class="jiesuan" bindtap="__e"><text>结算</text></view></view></view></view></block>