checkbox .wx-checkbox-input {
	border-radius: 50%;
	/* 		background-color: #ff4500;
	color: #FFFFFF !important; */
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
	border: 2rpx solid #ff4500;
	background: #ff4500;
	color: #FFFFFF !important;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
	font-size: 38rpx;
}
.gocart {
	width: 100%;
	margin-bottom: 80rpx;
}
.gocart-main {
	width: 100%;
	/* position: relative; */
}
.golist {
	padding: 30rpx 0 30rpx 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	border-bottom: 2rpx solid #F1F1F1;
}
.golist-l {
	width: 40rpx;
	height: 40rpx;
	margin-top: 100rpx;
	margin-left: 30rpx;
}
.golist-t {
	margin-left: 50rpx;
}
.golist-t image {
	width: 250rpx;
	height: 250rpx;
	margin: 0 auto;
}
.golist-r {
	width: 100%;
	margin-left: 20rpx;
}
.golist-r1 {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	margin-top: 30rpx;
}
.golist-r1-l {
	width: 250rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.golist-r1 image {
	width: 32rpx;
	height: 32rpx;
	margin-top: 6rpx;
	margin-left: 30rpx;
}
.golist-r-text1 {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #333333;
	font-size: 28rpx;
}
.golist-r-text2 {
	color: #FF3939;
	font-size: 30rpx;
