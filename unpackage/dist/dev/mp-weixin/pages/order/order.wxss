
.order {
	width: 100%;
	background-color: #F1F1F1;
}
.order-head {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-size: 30rpx;
