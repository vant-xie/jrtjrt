
.withdrawal {
	width: 100%;
	background-color: #F1F1F1;
	position: relative;
}

/* head */
.withdrawal-head {
	width: 610rpx;
	background-color: #FFFFFF;
	border-radius: 10rpx;
	margin: 0 auto;
	margin-top: 20rpx;
	padding: 40rpx 42rpx 42rpx 40rpx;
}
.withdrawal-head-text1 {
	font-size: 28rpx;
	color: #7C838D;
}
.withdrawal-head-text2 {
	font-size: 28rpx;
	color: #7C838D;
}
.withdrawal-head-item {
	font-size: 72rpx;
	color: #121922;
	border-bottom: 2rpx solid #E4E4EE;
	padding-bottom: 38rpx;
	padding-top: 38rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
}
.withdrawal-head-item>input{
	height: 96rpx;
}
.withdrawal-head-bottom {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding-top: 60rpx;
}
.withdrawal-head-btn {
	font-size: 28rpx;
	color: #14325B;
}

/* main */
.withdrawal-main {
	padding: 50rpx 30rpx 0 30rpx;
}
.withdrawal-main-text1 {
	font-size: 28rpx;
	color: #333333;
}
.withdrawal-main-text2 {
	font-size: 22rpx;
	color: #FF4444;
}
.withdrawal-main-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	margin-top: 26rpx;
	margin-bottom: 26rpx;
}
.withdrawal-main-icon1 {
	width: 100rpx;
	height: 100rpx;
}
.withdrawal-main-icon1>image {
	width: 100%;
	height: 100%;
}
.withdrawal-main-icon2 {
	width: 100rpx;
	height: 100rpx;
	margin-left: 20rpx;
}
.withdrawal-main-icon2>image {
	width: 100%;
	height: 100%;
}

/* footer */
.withdrawal-footer {
	width: 690rpx;
	height: 96rpx;
	line-height: 96rpx;
	border-radius: 48rpx;
	color: #FFFFFF;
	text-align: center;
	background-color: #FF4444;
	/* margin:0 auto; */
	position: absolute;
	left: 30rpx;
	bottom: 60rpx;
}

